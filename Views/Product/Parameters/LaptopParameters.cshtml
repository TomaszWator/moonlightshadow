        @string.Join(", ", ViewBag.Product.Features)
    </div>
</div>
<br />
<hr />

<div class="product-parameters-group">
    <div class="float-start m-0 p-0 fw-bold fs-4">Sterowanie</div>
    <br />
    <br />
    <div class="float-end text-end m-0 p-0 fw-bold">
        @string.Join(", ", ViewBag.Product.Control)
    </div>
</div>
